    "version": "0.2.0",
    "configurations": [
        {
            "console": "integratedTerminal",
            "name": "Launch Program",
            "env": {
                "ARRANGE_DATABASE_DIRECTORY": "./database/",
                "ARRANGE_PUBLIC_CERTIFICATE_FILE": "./pub.cert",
                "ARRANGE_PRIVATE_KEY_FILE": "./priv.key",
                "ARRANGE_HTTPS_PORT": "443",
                "ARRANGE_MODULES_PATH": "./modules/",
                "ARRANGE_TOKEN_SECRET": "hubbelebubbele"
            },
            "program": "${workspaceFolder}/arrange.mjs",
            "runtimeExecutable": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}